    "moment-timezone": "^0.5.31"
  },
  "devDependencies": {
    "typescript": "^3.9.5",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1"
  }
}
